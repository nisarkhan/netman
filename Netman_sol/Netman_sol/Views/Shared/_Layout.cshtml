<!DOCTYPE html>
<html ng-app="userApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @*@Styles.Render("~/bundles/assets/css")*@
    @*@Styles.Render("~/bundles/assets/css/fonts")*@

    @RenderSection("Css", false)

    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/assets/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/assets/css/sunloan.css" rel="stylesheet">
    <link href="~/assets/css/sunloan.datatable.css" rel="stylesheet">
    <link href="~/assets/css/sunloan.datepicker.css" rel="stylesheet">
    <link href="~/assets/css/sunloan.animate.css" rel="stylesheet">

    <!--ui-select-->
    <link href="~/assets/css/ui-select/select.css" rel="stylesheet" />
    <link href="~/assets/css/ui-select/select.default.css" rel="stylesheet" />
    <link href="~/assets/css/ui-select/select2.css" rel="stylesheet" />
    <link href="~/assets/css/tags-input/tags.css" rel="stylesheet" />
    <script src="~/assets/js/vendor/jquery.min.js"></script>
    <script src="~/assets/js/vendor/bootstrap.min.js"></script>

    <script src="~/assets/js/vendor/angular/angular.js"></script>
    <script src="~/assets/js/vendor/angular/angular-route.js"></script>
    <script src="~/assets/js/vendor/angular/angular-resource.js"></script>
    <script src="~/assets/js/vendor/angular/angular-animate.js"></script>
    <script src="~/assets/js/vendor/angular/angular-cookies.js"></script>


    <!--ui-select-->
    <script src="~/assets/js/ui-select/select.js"></script>
    <script src="~/assets/js/ui-select/angular-sanitize.js"></script>

    <!--tags input-->
    <script src="~/assets/js/tags-input/tags-input.js"></script>
    <link href="~/Assets/css/tags-input/tags.css" rel="stylesheet" />
</head>
<body>


    <sun-notification></sun-notification>
    @*<sun-header></sun-header>*@

    <!--header starts-->
    @Html.Partial("~/Views/Shared/partial/_headermvc.cshtml")


    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>

        </footer>
    </div>

    @*@Scripts.Render("~/bundles/jquery")*@

    @*@Scripts.Render("~/bundles/assets/js/vendor")
        @Scripts.Render("~/bundles/assets/js/vendor/angular")

        @Scripts.Render("~/bundles/common/directives")
        @Scripts.Render("~/bundles/app")*@

    <script src="~/scripts/customer.js"></script>

    <script src="~/scripts/common/directives/templates.js"></script>
    <script src="~/scripts/common/directives/gridview.js"></script>
    <script src="~/scripts/common/directives/dropdown.js"></script>
    <script src="~/scripts/common/directives/mask.js"></script>
    <script src="~/scripts/common/directives/form.js"></script>
    <script src="~/scripts/common/directives/match.js"></script>
    <script src="~/assets/js/plugins.js"></script>
    <script src="~/scripts/common/directives/auto-complete.js"></script>



    <!-- Services -->
    <script src="~/scripts/common/services/storage.js"></script>
    <script src="~/scripts/common/services/billtos.js"></script>
    <script src="~/Scripts/common/services/api_geo_names.js"></script>

    @RenderSection("scripts", required: false)

</body>
</html>
